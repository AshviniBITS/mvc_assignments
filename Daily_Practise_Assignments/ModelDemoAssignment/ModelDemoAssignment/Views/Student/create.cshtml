
@{
    ViewBag.Title = "create";
}

<h2>create Student</h2>
<form action="/Product/create" method="post">
    <div class="form-group">
        <label>"Student Id" </label>
        <input type="text" class="form-control" id="txtproductid" placeholder="Student Id" name="StudentId" />


    </div>
    <div class="form-group">
        <label>"Student Name" </label>
        <input type="text" class="form-control" id="txtproductname" placeholder="Student Name" name="StudentName" />


    </div>
    <div class="form-group">
        <label>"D.NO" </label>
        <input type="text" class="form-control" id="txtdno" placeholder="D.no" name="Dno" />


    </div>
    <div class="form-group">
        <label>"street" </label>
        <input type="text" class="form-control" id="txtstreet" placeholder="Street" name="street" />


    </div>
    <div class="form-group">
        <label>"landmark" </label>
        <input type="text" class="form-control" id="txtlandmark" placeholder="landmark" name="landmark" />


    </div>
    <div class="form-group">
        <label>"City" </label>
        <input type="text" class="form-control" id="txtcity" placeholder="City" name="city" />


    </div>
    <button>Submit</button>
</form>



