@model CollegeManagementProject.Models.Teacher
@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_LayoutPage1.cshtml";
}


<h2>Details</h2>
<table class="table">
    <tr>
        <th>Teacher Id</th>
        <td>@Model.teacherId</td>
    </tr>
    <tr>
        <th>Teacher Name</th>
        <td>@Model.teacherName</td>
    </tr>
    
    <tr>
        <th>Date of joining</th>
        <td>@Model.DateOfJoining</td>
    </tr>
    <tr>
        <th>Email</th>
        <td>@Model.Email</td>
    </tr>
    <tr>
        <th>Mobile no</th>
        <td>@Model.PhoneNo</td>
    </tr>
    <tr>
        <th>Address</th>
        <td>@Model.Address</td>
    </tr>
    <tr>
        <th>Department Id</th>
        <td>
            @if (Model.Department != null)
            {

                @Model.Department.DeptName
            }
        </td>
    </tr>
  

</table>

<a href="/Teacher/index">Back to Teachers Dashboard</a>

